{
  "name": "note-taker",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "codegen": "npm-run-all -p codegen:*",
    "codegen:graphql": "graphql-codegen",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx",
    "type-check": "tsc",
    "qa": "npm-run-all -p lint test type-check",
    "ci-build": "yarn qa && yarn codegen && yarn build",
    "postinstall": "yarn codegen"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn qa",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "transform": {
      "^.+\\.(graphql|gql)$": "jest-transform-graphql",
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.41",
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@graphql-modules/core": "^0.7.15",
    "@graphql-modules/di": "^0.7.15",
    "apollo-datasource": "^0.7.0",
    "apollo-server-caching": "^0.5.1",
    "apollo-server-micro": "^2.14.2",
    "datasource-sql": "^1.1.1",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "micro-cors": "^0.1.1",
    "mysql": "^2.18.1",
    "next": "^9.3.4",
    "next-with-apollo": "^5.0.0",
    "node-fetch": "^2.6.0",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-tools": "^1.7.1",
    "tsdef": "^0.0.13"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/fragment-matcher": "^1.13.1",
    "@graphql-codegen/schema-ast": "^1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/typescript-operations": "^1.13.1",
    "@graphql-codegen/typescript-resolvers": "^1.13.1",
    "@strv/eslint-config-react": "^3.1.1",
    "@strv/eslint-config-typescript": "^2.3.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "^13.9.8",
    "@types/ramda": "^0.27.3",
    "@types/react": "^16.9.31",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.2.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "husky": ">=4",
    "jest": "^25.2.7",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": ">=10",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "waait": "^1.0.5"
  }
}
